{
  "name": "git-line-blame",
  "displayName": "Git Line Blame",
  "description": "Display inline information in the text editor about the latest commit that edited the currently selected line",
  "publisher": "carlthome",
  "version": "0.8.0",
  "license": "AGPL-3.0-only",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other",
    "SCM Providers"
  ],
  "keywords": [
    "git",
    "blame"
  ],
  "bugs": {
    "url": "https://github.com/carlthome/vscode-git-line-blame/issues"
  },
  "homepage": "https://github.com/carlthome/vscode-git-line-blame",
  "repository": {
    "type": "git",
    "url": "https://github.com/carlthome/vscode-git-line-blame.git"
  },
  "contributes": {},
  "activationEvents": [
    "workspaceContains:.git"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "deploy": "vsce publish"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "@types/vscode": "^1.88.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vscode/test-electron": "^2.3.9",
    "@vscode/vsce": "^2.26.0",
    "eslint": "^9.1.1",
    "glob": "^10.3.12",
    "mocha": "^10.4.0",
    "typescript": "^5.4.5"
  }
}
